{
    "name": "tustin/psn-php",
    "license": "MIT",
    "description": "PHP wrapper for the PlayStation API.",
    "homepage": "https://github.com/Tustin/psn-php",
    "keywords": ["playstation", "api", "php"],
    "autoload": {
        "psr-4": {
            "Tustin\\PlayStation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Tustin\\PlayStation\\Tests\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1"
    }
}
